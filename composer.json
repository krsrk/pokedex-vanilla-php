{
    "name": "pokedex/pokedex-vanilla-php",
    "description": "Pokedex Dev Project. Vanilla PHP",
    "keywords": ["pokedex", "pokedex", "vanilla-php"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1||^8.2",
        "twig/twig": "^v3.6.0",
        "illuminate/database": "^v10.10.1",
        "vlucas/phpdotenv": "^v5.5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Pokedex\\": "src/Pokedex",
            "Utils\\": "src/Utils"
        },
        "files": [
            "src/Utils/helpers.php"
        ]
    },
    "require-dev": {
        "pestphp/pest": "^2.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
